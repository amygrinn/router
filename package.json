{
  "name": "@tygr/router",
  "version": "1.0.0",
  "description": "Router for tygr",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "sass"
  ],
  "browserslist": [
    "defaults"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "alloy",
      "alloy/react",
      "alloy/typescript"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "eol-last": [
        "error",
        "always"
      ],
      "no-undef": 0,
      "max-params": [
        "error",
        4
      ],
      "no-param-reassign": 0,
      "@typescript-eslint/prefer-optional-chain": 0
    },
    "ignorePatterns": [
      "lib/*",
      "dist/*",
      "webpack.config.js",
      "html-webpack-exclude-tags-plugin.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "test": "mocha tests/**/*.spec.js --exit",
    "test:w": "npm run test -- -w",
    "clean": "rimraf lib dist",
    "ts": "tsc",
    "ts:w": "npm run ts -- -w",
    "demo": "webpack --config demo/webpack.config",
    "demo:w": "webpack-dev-server --config demo/webpack.config",
    "performance": "webpack --config performance/webpack.config",
    "build": "run-s clean ts demo",
    "serve": "http-server dist",
    "start": "npm-run-all ts -p ts:w demo:w",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run lint:fix",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "prettier": "npm run prettier:fix",
    "quality:check": "run-s --print-label --silent lint:check prettier:check",
    "quality:fix": "run-p --print-label --silent lint:fix prettier:fix",
    "quality": "npm run quality:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylergrinn/tygr-router.git"
  },
  "keywords": [
    "react",
    "login",
    "webpack"
  ],
  "author": "Tyler Grinn",
  "license": "ISC",
  "peerDependencies": {
    "react": "17"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.12.5",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-alloy": "^3.8.0",
    "eslint-plugin-react": "^7.21.2",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "mocha": "^8.2.1",
    "node-sass-json-importer": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.6.2",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "serve-static": "^1.14.1",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.3",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/register": "^7.12.1",
    "@tygr/switch": "^1.1.1"
  }
}
