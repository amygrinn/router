{
  "name": "@tygr/router",
  "version": "1.0.3",
  "description": "Router for tygr",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "sass",
    "_index.scss"
  ],
  "browserslist": [
    "defaults"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "alloy",
      "alloy/react",
      "alloy/typescript"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "eol-last": [
        "error",
        "always"
      ],
      "no-undef": 0,
      "max-params": [
        "error",
        4
      ],
      "no-param-reassign": 0,
      "@typescript-eslint/prefer-optional-chain": 0
    },
    "ignorePatterns": [
      "lib/*",
      "dist/*",
      "webpack.config.js",
      "output"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "test": "mocha tests/**/*.spec.js --exit",
    "test:w": "npm run test -- -w",
    "clean": "rimraf lib dist",
    "ts": "tsc",
    "ts:w": "npm run ts -- -w",
    "demo": "webpack --config demo/webpack.config",
    "demo:w": "webpack-dev-server --config demo/webpack.config",
    "performance": "webpack --config performance/webpack.config",
    "build": "run-s clean ts demo",
    "serve": "http-server dist",
    "start": "npm-run-all ts -p ts:w demo:w",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run lint:fix",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "prettier": "npm run prettier:fix",
    "quality:check": "run-s --print-label --silent lint:check prettier:check",
    "quality:fix": "run-p --print-label --silent lint:fix prettier:fix",
    "quality": "npm run quality:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylergrinn/tygr-router.git"
  },
  "keywords": [
    "react",
    "login",
    "webpack"
  ],
  "author": "Tyler Grinn",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.12.5",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-react": "^7.21.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mocha": "^9.2.2",
    "node-sass-json-importer": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.6.2",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^12.6.0",
    "serve-static": "^1.14.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.0.3",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
